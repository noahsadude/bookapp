<!DOCTYPE html>
<html lang="en">
<head>
  <title>Document</title>
  <%- include('partials/head') %>
</head>
<body>
  <header>
    <%- include('partials/header') %>  
  </header>
  <main>
    <h4>Search Results</h4>
    <% searchResults.forEach( book => { %>
      <section class="book-info">
        <%- include('partials/detail', {book: book}) %>
        <button><a href="/add">Select this book</a></button>
      </section>

    <% }) %>
  </main>
  <footer>
    <%- include('partials/footer') %>
  </footer>
</body>
</html>